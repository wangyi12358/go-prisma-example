import "./user.api"

type (
	LoginReq {
		Username string `json:"username"`
		Passwrod string `json:"password"`
	}
	LoginReply {
		Token string `json:"token"`
	}
)

service Admin {
	@handler login
	post /api/login(LoginReq) returns (LoginReply)
}

@server(
	jwt: Auth
)
service Admin {
	@handler logout
	post /api/logout() returns ()
}